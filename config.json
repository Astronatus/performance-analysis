{
  "plugins": {
    "add": "analysisstorer"
  },
  "utc": true,
  "browsertime": {
    "browser": "chrome",
    "iterations": 3,
    "video": false,
    "visualMetrics": false,
    "screenshot": true,
    "screenshotParams": {
      "type": "png",
      "png": {
        "compressionLevel": 6
      }
    },
    "viewPort": "390x844",
    "userAgent": "Mozilla/5.0 (iPhone; CPU iPhone OS 14_0 like Mac OS X) AppleWebKit/605.1.15 (KHTML, like Gecko) Version/14.0 Mobile/15A5341f Safari/604.1",
    "chrome": {
      "mobileEmulation": {
        "enabled": true,
        "deviceMetrics": {
          "width": 390,
          "height": 844,
          "pixelRatio": 3
        }
      },
      "args": [
        "--window-size=390,844",
        "--force-device-scale-factor=3",
        "--no-first-run"
      ],
      "CPUThrottlingRate": 2
    },
    "connectivity": {
      "engine": "throttle",
      "profile": "4g"
    }
  }
}
