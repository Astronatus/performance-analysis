{
  "plugins": {
    "add": "analysisstorer"
  },
  "utc": true,
  "browsertime": {
    "browser": "chrome",
    "iterations": 1,
    "video": true,
    "visualMetrics": false,
    "screenshot": true,
    "screenshotParams": {
      "type": "png",
      "png": {
        "compressionLevel": 6
      }
    },
    "viewPort": "360x740",
    "userAgent": "Mozilla/5.0 (Linux; Android 14; Pixel 7) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/121.0.6167.164 Mobile Safari/537.36",
    "chrome": {
      "mobileEmulation": {
        "width": 360,
        "height": 740,
        "pixelRatio": 1
      },
      "args": [
        "--window-position=0,0",
        "--force-device-scale-factor=1",
        "--high-dpi-support=1",
        "--no-first-run"
      ],
      "CPUThrottlingRate": 2
    },
    "connectivity": {
      "engine": "throttle",
      "profile": "custom",
      "down": 12000,
      "up": 12000,
      "rtt": 85
    }
  }
}
